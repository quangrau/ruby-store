<!DOCTYPE html>
<html>
<head>
  <title>RubyStore | Just a tiny app</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="navbar navbar-default navbar-fixed-top">
    <div class="container">
      <div class="navbar-header">
        <a href="../" class="navbar-brand">RubyStore</a>
        <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#navbar-main">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
      </div>
      <div class="navbar-collapse collapse" id="navbar-main">
        <ul class="nav navbar-nav pull-right">
          <li class="dropdown">
            <a class="dropdown-toggle" data-toggle="dropdown" href="javascript:;" id="menu">Menu <span class="caret"></span></a>
            <ul class="dropdown-menu" aria-labelledby="menu">
              <li><a tabindex="-1" href="/admin/category">Categories</a></li>
              <li class="divider"></li>
              <li><a tabindex="-1" href="/admin/product">Products</a></li>
              <li class="divider"></li>
              <li><a tabindex="-1" href="/admin/user">Users</a></li>

            </ul>
          </li>
          <li>
            <a href=""><i class=""></i>Help</a>
          </li>
        </ul>
      </div>
    </div>
  </div>

  <%= yield %>

</body>
</html>
